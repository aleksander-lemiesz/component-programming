      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane fx:id="pane" prefHeight="480.0" prefWidth="720.0">
         <children>
            <Canvas fx:id="canvas" height="450.0" layoutX="14.0" layoutY="15.0" onMouseClicked="#canvasMouseClicked" width="450.0" />
            <Button fx:id="in8" layoutX="594.0" layoutY="263.0" mnemonicParsing="false" onAction="#buttonOnAction" text="8" />
            <Button fx:id="in4" layoutX="553.0" layoutY="225.0" mnemonicParsing="false" onAction="#buttonOnAction" text="4" />
            <Button fx:id="in6" layoutX="635.0" layoutY="225.0" mnemonicParsing="false" onAction="#buttonOnAction" text="6" />
            <Button fx:id="in7" layoutX="553.0" layoutY="263.0" mnemonicParsing="false" onAction="#buttonOnAction" text="7" />
            <Button fx:id="in2" layoutX="594.0" layoutY="188.0" mnemonicParsing="false" onAction="#buttonOnAction" text="2" />
            <Button fx:id="in3" layoutX="635.0" layoutY="188.0" mnemonicParsing="false" onAction="#buttonOnAction" text="3" />
            <Button fx:id="in5" layoutX="594.0" layoutY="225.0" mnemonicParsing="false" onAction="#buttonOnAction" text="5" />
            <Button fx:id="in9" layoutX="635.0" layoutY="263.0" mnemonicParsing="false" onAction="#buttonOnAction" text="9" />
            <Button fx:id="in1" layoutX="553.0" layoutY="188.0" mnemonicParsing="false" onAction="#buttonOnAction" text="1" />
            <Button fx:id="saveButton" layoutX="590.0" layoutY="440.0" mnemonicParsing="false" onAction="#saveSudokuToFile" text="%save" />
            <Button fx:id="loadButton" layoutX="646.0" layoutY="440.0" mnemonicParsing="false" onAction="#loadSudokuFromFile" text="%load" />
         </children>
      </Pane>
   </children>
</GridPane>
